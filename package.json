  "repository": {
    "type": "git",
    "url": "https://github.com/thilinatpr/ticker-backend-cloudflare-worker.git"
  },
  "bugs": {
    "url": "https://github.com/thilinatpr/ticker-backend-cloudflare-worker/issues"
  },
  "homepage": "https://github.com/thilinatpr/ticker-backend-cloudflare-worker#readme",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "npm run setup-queue && wrangler deploy",
    "deploy:production": "npm run setup-queue && wrangler deploy --env production",
    "setup-queue": "wrangler queues create ticker-dividend-queue || echo 'Queue already exists or creation failed - continuing...'",
    "tail": "wrangler tail ticker-backend-worker",
    "tail:pretty": "wrangler tail ticker-backend-worker --format=pretty",
    "whoami": "wrangler whoami",
    "test": "echo 'No tests specified yet'"
  },
  "devDependencies": {
    "wrangler": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
