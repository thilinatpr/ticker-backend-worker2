{
  "name": "ticker-backend-worker2",
  "version": "1.0.0",
  "type": "module",
  "description": "Cloudflare Worker for reliable ticker backend cron job scheduling and processing",
  "main": "src/index.js",
  "keywords": [
    "cloudflare",
    "worker",
    "cron",
    "stocks",
    "dividends",
    "scheduler",
    "api"
  ],
  "author": "Your Name",
